// См. УправлениеДоступомПереопределяемый.ПриЗаполненииВидовДоступа.
Процедура ПриЗаполненииВидовДоступа(ВидыДоступа) Экспорт
	
	//
	//ВидДоступа = ВидыДоступа.Добавить();
	//ВидДоступа.Имя = "ГруппыФизическихЛиц";
	//ВидДоступа.Представление    = НСтр("ru='Группы физических лиц';uk='Групи фізичних осіб'");
	//ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.ФизическиеЛица");
	//ВидДоступа.ТипГруппЗначений = Тип("СправочникСсылка.ГруппыДоступаФизическихЛиц");

	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "Организации";
	ВидДоступа.Представление    = НСтр("ru = 'Организации'; uk = 'Організації'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.Организации");
		
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "ПодразделенияКомпании";
	ВидДоступа.Представление    = НСтр("ru = 'Подразделения'; uk = 'Підрозділи'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.ПодразделенияКомпании");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "ТорговыеПлощадки";
	ВидДоступа.Представление    = НСтр("ru = 'Объекты'; uk = 'Об''єкти'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.ТорговыеПлощадки");

	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "ТипыЦен";
	ВидДоступа.Представление    = НСтр("ru = 'Типы цен'; uk = 'Типи цін'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.ТипыЦен");
	
	ВидДоступа = ВидыДоступа.Добавить();
	ВидДоступа.Имя = "Менеджеры";
	ВидДоступа.Представление    = НСтр("ru = 'Менеджеры'; uk = 'Менеджери'");
	ВидДоступа.ТипЗначений      = Тип("СправочникСсылка.ФизическиеЛица");
	
КонецПроцедуры

// См. УправлениеДоступомПереопределяемый.ПриЗаполненииИспользованияВидаДоступа.
Процедура ПриЗаполненииИспользованияВидаДоступа(ВидДоступа, Использование) Экспорт
	
	УстановитьПривилегированныйРежим(Истина);
	//
	//Если ВидДоступа = "ГруппыФизическихЛиц" Тогда
	//	Использование = Константы.ОграничиватьДоступПоФизическимЛицам.Получить();
	//КонецЕсли;
	
	Если ВидДоступа = "Организации"
		ИЛИ ВидДоступа = "ПодразделенияКомпании"
		ИЛИ ВидДоступа = "ТорговыеПлощадки" 
		ИЛИ ВидДоступа = "Менеджеры" Тогда
		
		Использование = Истина;
		
	ИначеЕсли ВидДоступа = "ТипыЦен" Тогда 
		
		Использование = Истина;
		
	КонецЕсли;
	
	УстановитьПривилегированныйРежим(Ложь);
	
КонецПроцедуры

