				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
			<attributeUseRestriction>
				<field>true</field>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</attributeUseRestriction>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПредставлениеСотрудникЗУП</dataPath>
			<field>ПредставлениеСотрудникЗУП</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Представление сотрудник ЗУП</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Анкета.Дата КАК Дата,
	Анкета.Ссылка КАК Анкета,
	Анкета.ПодразделениеКомпании КАК ПодразделениеКомпании,
	Анкета.Респондент КАК Респондент,
	Анкета.Интервьюер КАК Интервьюер,
	Анкета.Реализатор КАК Реализатор,
	Анкета.Реализатор.Код КАК ИНН,
	Анкета.ШаблонАнкеты КАК ШаблонАнкеты,
	ТП_ВидПлощадки.Значение КАК ВидПлощадки,
	ТП_ГрафикРаботы.Значение КАК ГрафикРаботы,
	ЕСТЬNULL(ТП_Кураторы.Значение, ЗНАЧЕНИЕ(Справочник.ФизическиеЛица.ПустаяСсылка)) КАК КураторПлощадки,
	Анкета.Респондент.Global_ID КАК Global_ID,
	Анкета.ПроцентВыполнения КАК ПроцентВыполнения,
	Анкета.ПроцентВыполнения &gt;= &amp;ПроцентПрохождения КАК Успешно
ПОМЕСТИТЬ втАнкеты
ИЗ
	Документ.Анкета КАК Анкета
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ТорговыеПлощадки.ДополнительныеРеквизиты КАК ТП_ВидПлощадки
		ПО Анкета.Респондент = ТП_ВидПлощадки.Ссылка
			И (ТП_ВидПлощадки.Свойство.Имя = "ВидПлощадки")
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ТорговыеПлощадки.ДополнительныеРеквизиты КАК ТП_ГрафикРаботы
		ПО Анкета.Респондент = ТП_ГрафикРаботы.Ссылка
			И (ТП_ГрафикРаботы.Свойство.Имя = "ГрафикРаботы")
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ТорговыеПлощадки.ДополнительныеРеквизиты КАК ТП_Кураторы
		ПО Анкета.Респондент = ТП_Кураторы.Ссылка
			И (ТП_Кураторы.Свойство.Имя = "КураторПлощадки")
ГДЕ
	Анкета.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И Анкета.Проведен
	И Анкета.РежимАнкетирования = ЗНАЧЕНИЕ(Перечисление.РежимыАнкетирования.Интервью)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВложенныйЗапрос.ШаблонАнкеты КАК ШаблонАнкеты,
	ШаблоныАнкетПремии.ПодразделениеКомпании КАК ПодразделениеКомпании,
	МАКСИМУМ(ШаблоныАнкетПремии.Премия) КАК Премия
ПОМЕСТИТЬ втПремии
ИЗ
	(ВЫБРАТЬ
		втАнкеты.ШаблонАнкеты КАК ШаблонАнкеты
	ИЗ
		втАнкеты КАК втАнкеты
	
	СГРУППИРОВАТЬ ПО
		втАнкеты.ШаблонАнкеты) КАК ВложенныйЗапрос
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ШаблоныАнкет.Премии КАК ШаблоныАнкетПремии
		ПО ВложенныйЗапрос.ШаблонАнкеты = ШаблоныАнкетПремии.Ссылка

СГРУППИРОВАТЬ ПО
	ВложенныйЗапрос.ШаблонАнкеты,
	ШаблоныАнкетПремии.ПодразделениеКомпании
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втАнкеты.Дата КАК Дата,
	втАнкеты.Анкета КАК Анкета,
	втАнкеты.Реализатор КАК Реализатор,
	втДанныеЗУП.ПредставлениеСотрудникЗУП КАК ПредставлениеСотрудникЗУП,
	ВЫБОР
		КОГДА НЕ ФизическиеЛица.Ссылка ЕСТЬ NULL
			ТОГДА ФизическиеЛица.Ссылка
		КОГДА НЕ втДанныеЗУП.Сотрудник ЕСТЬ NULL
			ТОГДА втДанныеЗУП.Сотрудник
		ИНАЧЕ ""
	КОНЕЦ КАК СотрудникЗУП,
	втАнкеты.ИНН = втДанныеЗУП.КодСотрудник КАК СотрудникОтпустилЧек
ПОМЕСТИТЬ втПроверкаСотрудники
ИЗ
	втАнкеты КАК втАнкеты
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ втДанныеЗУП КАК втДанныеЗУП
			ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ФизическиеЛица КАК ФизическиеЛица
			ПО втДанныеЗУП.КодСотрудник = ФизическиеЛица.Код
		ПО втАнкеты.Global_ID = втДанныеЗУП.КодОбъекта
			И (втАнкеты.Дата МЕЖДУ втДанныеЗУП.ДатаПриход И втДанныеЗУП.ДатаУход)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втПроверкаСотрудники.Анкета КАК Анкета,
	МАКСИМУМ(втПроверкаСотрудники.СотрудникОтпустилЧек) КАК Проверка
ПОМЕСТИТЬ РезультатыПроверкиСотрудников
ИЗ
	втПроверкаСотрудники КАК втПроверкаСотрудники

СГРУППИРОВАТЬ ПО
	втПроверкаСотрудники.Анкета
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втАнкеты.Дата КАК Дата,
	втАнкеты.Анкета КАК Анкета,
	втАнкеты.Респондент КАК Респондент,
	втАнкеты.Интервьюер КАК Интервьюер,
	втАнкеты.Реализатор КАК Реализатор,
	ОтветыНаВопросыАнкет.Вопрос КАК Вопрос,
	ОтветыНаВопросыАнкет.ЭлементарныйВопрос КАК ЭлементарныйВопрос,
	ОтветыНаВопросыАнкет.ЭлементарныйВопрос.Формулировка КАК Формулировка,
	ОтветыНаВопросыАнкет.ЭлементарныйВопрос.Родитель КАК Группа,
	ОтветыНаВопросыАнкет.ЭлементарныйВопрос.Родитель.ВидРасчетаИтогов КАК ВидРасчетаИтога,
	ВЫБОР
		КОГДА ОтветыНаВопросыАнкет.ЭлементарныйВопрос.ТипОтвета = ЗНАЧЕНИЕ(Перечисление.ТипыОтветовНаВопрос.Булево)
			ТОГДА ОтветыНаВопросыАнкет.Ответ
		ИНАЧЕ ВЫРАЗИТЬ(ОтветыНаВопросыАнкет.Ответ КАК ЧИСЛО(1, 0))
	КОНЕЦ КАК Оценка,
	ОтветыНаВопросыАнкет.ОткрытыйОтвет КАК Комментарий,
	ВЫБОР
		КОГДА ОтветыНаВопросыАнкет.ЭлементарныйВопрос.ТипОтвета = ЗНАЧЕНИЕ(Перечисление.ТипыОтветовНаВопрос.Число)
			ТОГДА ОтветыНаВопросыАнкет.ЭлементарныйВопрос.МаксимальноеЗначение
		ИНАЧЕ 0
	КОНЕЦ КАК МаксЗначение,
	втАнкеты.ШаблонАнкеты КАК ШаблонАнкеты,
	втАнкеты.ВидПлощадки КАК ВидПлощадки,
	втАнкеты.ГрафикРаботы КАК ГрафикРаботы,
	втАнкеты.КураторПлощадки КАК КураторПлощадки,
	ОтветыНаВопросыАнкет.ЭлементарныйВопрос.ТипОтвета = ЗНАЧЕНИЕ(Перечисление.ТипыОтветовНаВопрос.Булево) КАК ВопросБулево,
	ОтветыНаВопросыАнкет.ЭлементарныйВопрос.ТипОтвета = ЗНАЧЕНИЕ(Перечисление.ТипыОтветовНаВопрос.Число) КАК ВопросЧисло,
	ОтветыНаВопросыАнкет.Вопрос.Родитель.Код * 1000 + ОтветыНаВопросыАнкет.Вопрос.Код КАК Порядок,
	втАнкеты.ИНН КАК ИНН,
	ВЫБОР
		КОГДА НЕ ПараметрыВопросовПодразделения.Коэффициент ЕСТЬ NULL
			ТОГДА ПараметрыВопросовПодразделения.Коэффициент
		КОГДА НЕ ПараметрыВопросовОбщие.Коэффициент ЕСТЬ NULL
			ТОГДА ПараметрыВопросовОбщие.Коэффициент
		ИНАЧЕ 1
	КОНЕЦ КАК КоэффициентВопроса,
	ОтветыНаВопросыАнкет.ЭлементарныйВопрос.Родитель.Коэффициент / 100 КАК ПроцентГруппы,
	ВЫБОР
		КОГДА НЕ втАнкеты.Успешно
				И ОтветыНаВопросыАнкет.ЭлементарныйВопрос.ТипОтвета = ЗНАЧЕНИЕ(Перечисление.ТипыОтветовНаВопрос.Число)
				И ОтветыНаВопросыАнкет.Ответ = 0
			ТОГДА ВЫБОР
					КОГДА НЕ ПараметрыВопросовПодразделения.Штраф ЕСТЬ NULL
						ТОГДА ПараметрыВопросовПодразделения.Штраф
					КОГДА НЕ ПараметрыВопросовОбщие.Штраф ЕСТЬ NULL
						ТОГДА ПараметрыВопросовОбщие.Штраф
					ИНАЧЕ 0
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК Штрафы,
	0 КАК Премии,
	втАнкеты.ПроцентВыполнения КАК ПроцентВыполнения,
	втАнкеты.Успешно КАК Успешно,
	втАнкеты.Global_ID КАК Global_ID,
	ЕСТЬNULL(РезультатыПроверкиСотрудников.Проверка, ЛОЖЬ) КАК Проверка
ПОМЕСТИТЬ втНачисления
ИЗ
	втАнкеты КАК втАнкеты
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.ОтветыНаВопросыАнкет КАК ОтветыНаВопросыАнкет
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ТК_ПараметрыВопросовАнкет КАК ПараметрыВопросовОбщие
			ПО ОтветыНаВопросыАнкет.ЭлементарныйВопрос = ПараметрыВопросовОбщие.ЭлементарныйВопрос
				И (ПараметрыВопросовОбщие.ПодразделениеКомпании = ЗНАЧЕНИЕ(Справочник.ПодразделенияКомпании.ПустаяСсылка))
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ТК_ПараметрыВопросовАнкет КАК ПараметрыВопросовПодразделения
			ПО ОтветыНаВопросыАнкет.ЭлементарныйВопрос = ПараметрыВопросовПодразделения.ЭлементарныйВопрос
				И ОтветыНаВопросыАнкет.Регистратор.ПодразделениеКомпании = ПараметрыВопросовПодразделения.ПодразделениеКомпании
		ПО втАнкеты.Анкета = ОтветыНаВопросыАнкет.Анкета
			И (ОтветыНаВопросыАнкет.НомерЯчейки = 0)
		ЛЕВОЕ СОЕДИНЕНИЕ РезультатыПроверкиСотрудников КАК РезультатыПроверкиСотрудников
		ПО втАнкеты.Анкета = РезультатыПроверкиСотрудников.Анкета

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	втАнкеты.Дата,
	втАнкеты.Анкета,
	втАнкеты.Респондент,
	втАнкеты.Интервьюер,
	втАнкеты.Реализатор,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	втАнкеты.ШаблонАнкеты,
	втАнкеты.ВидПлощадки,
	втАнкеты.ГрафикРаботы,
	втАнкеты.КураторПлощадки,
	NULL,
	NULL,
	NULL,
	втАнкеты.ИНН,
	NULL,
	NULL,
	0,
	ВЫБОР
		КОГДА втАнкеты.ПроцентВыполнения &gt;= &amp;ПроцентПремирования
			ТОГДА ВЫБОР
					КОГДА НЕ ПремииПодразделения.Премия ЕСТЬ NULL
						ТОГДА ПремииПодразделения.Премия
					КОГДА НЕ ПремииОбщие.Премия ЕСТЬ NULL
						ТОГДА ПремииОбщие.Премия
					ИНАЧЕ 0
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ,
	втАнкеты.ПроцентВыполнения,
	втАнкеты.Успешно,
	втАнкеты.Global_ID,
	ЕСТЬNULL(РезультатыПроверкиСотрудников.Проверка, ЛОЖЬ)
ИЗ
	втАнкеты КАК втАнкеты
		ЛЕВОЕ СОЕДИНЕНИЕ РезультатыПроверкиСотрудников КАК РезультатыПроверкиСотрудников
		ПО втАнкеты.Анкета = РезультатыПроверкиСотрудников.Анкета
		ЛЕВОЕ СОЕДИНЕНИЕ втПремии КАК ПремииОбщие
		ПО втАнкеты.ШаблонАнкеты = ПремииОбщие.ШаблонАнкеты
			И (ПремииОбщие.ПодразделениеКомпании = ЗНАЧЕНИЕ(Справочник.ПодразделенияКомпании.ПустаяСсылка))
		ЛЕВОЕ СОЕДИНЕНИЕ втПремии КАК ПремииПодразделения
		ПО втАнкеты.ШаблонАнкеты = ПремииПодразделения.ШаблонАнкеты
			И втАнкеты.ПодразделениеКомпании = ПремииПодразделения.ПодразделениеКомпании
ГДЕ
	втАнкеты.Успешно
;

////////////////////////////////////////////////////////////////////////////////
