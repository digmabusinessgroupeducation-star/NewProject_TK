	<totalField>
		<dataPath>МаржаБезНДС</dataPath>
		<expression>ВЫБОР
    КОГДА ЕСТЬNULL(Сумма(СуммаУпр)-Сумма(СуммаНДС), 0) = 0
        ТОГДА 0
    ИНАЧЕ (Сумма(СуммаУпр)-Сумма(СуммаНДС) -Сумма(СебестоимостьУпр)) / (Сумма(СуммаУпр)-Сумма(СуммаНДС)) * 100
КОНЕЦ</expression>
	</totalField>
	<totalField>
		<dataPath>МаржаСНДС</dataPath>
		<expression>ВЫБОР
    КОГДА ЕСТЬNULL(Сумма(СуммаУпр), 0) = 0
        ТОГДА 0
    ИНАЧЕ (Сумма(СуммаУпр) -Сумма(СебестоимостьУпр)) / (Сумма(СуммаУпр)) *100
КОНЕЦ</expression>
	</totalField>
